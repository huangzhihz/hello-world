<!-- <!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 
	2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd" > <web-app> <display-name>Archetype 
	Created Web Application</display-name> </web-app> -->
<?xml version = "1.0" encoding = "UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaaee" xmlns:xsi="http://www.w3.org/200/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<servlet>
		<servlet-name>cjservletMVC</servlet-name>
		<servlet-class>honstat.spring.service.impl.CJDispatcherServlet
		</servlet-class>
		<!-- 设置初始化的文件 -->
		<init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>spring/application.properties</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>cjservletMVC</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>